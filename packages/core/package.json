{
  "name": "@xiaohe01/stylelint-config",
  "type": "module",
  "version": "2.4.0",
  "description": "ðŸ¤š Stylelint config preset for xiaohe",
  "author": "xiaohe0601 <xiaohe0601@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/xiaohe0601/xiaohe-stylelint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaohe0601/xiaohe-stylelint-config.git"
  },
  "bugs": "https://github.com/xiaohe0601/xiaohe-stylelint-config/issues",
  "keywords": [
    "stylelint-config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown"
  },
  "peerDependencies": {
    "stylelint": ">=16.13.0"
  },
  "dependencies": {
    "local-pkg": "catalog:node",
    "postcss-html": "catalog:style",
    "postcss-scss": "catalog:style",
    "stylelint-config-recess-order": "catalog:lint",
    "stylelint-order": "catalog:lint",
    "stylelint-scss": "catalog:lint"
  },
  "devDependencies": {
    "stylelint": "catalog:lint"
  }
}